{
	"processors": [],
	"extends": [
		"stylelint-config-recommended"
	],
	"syntax": "scss",
	"rules": {
		"block-no-empty": null,
		"color-no-invalid-hex": true,
		"comment-empty-line-before": [
			"always",
			{
				"ignore": [
					"stylelint-commands",
					"after-comment"
				]
			}
		],
		"declaration-colon-space-after": "always",
		"indentation": [
			"tab",
			{
				"except": [
					"value"
				]
			}
		],
		"max-empty-lines": 2,
		"no-descending-specificity": null,
		"rule-empty-line-before": [
			"always",
			{
				"except": [
					"first-nested"
				],
				"ignore": [
					"after-comment"
				]
			}
		],
		"selector-pseudo-element-no-unknown": null,
		"unit-whitelist": [
			"em",
			"rem",
			"%",
			"vh",
			"vw",
			"px",
			"fr"
		]
	}
}